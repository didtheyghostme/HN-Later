{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["chrome"]
  },
  "include": ["entrypoints/**/*.ts", "entrypoints/**/*.tsx", "wxt.config.ts", "components/**/*.ts", "components/**/*.tsx", "assets/**/*.css"]
}


